<% @documents.each do |document| %>
<% show_for document do |d| %>
  <tr>
    <td><%= d.attribute :name %></td>
    <td><%= d.attribute :in_date %></td>
    <td><%= d.attribute :out_date %></td>
    <td><%= d.attribute :receive_date %></td>
    <td><%= d.attribute :description %></td>
    <td><%= d.attribute :status_name %></td>
    <td><%= link_to 'Show', document %></td>
    <td><%= link_to 'Edit', edit_document_path(document) %></td>
    <td><%= link_to 'Destroy', document, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
<% end %>
