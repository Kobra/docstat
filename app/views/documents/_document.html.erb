<% @documents.each do |document| %>
<% show_for document do |d| %>
  <tr>
    <td><%= d.attribute :name, :label => false %></td>
    <td><%= d.attribute :in_date, :label => false %></td>
    <td><%= d.attribute :out_date, :label => false %></td>
    <td><%= d.attribute :receive_date, :label => false %></td>
    <td><%= d.attribute :description, :label => false %></td>
    <td><%= d.attribute :status_name, :label => false %></td>
    <td><%= link_to t(:show), document %></td>
    <td><%= link_to t(:edit), edit_document_path(document) %></td>
    <td><%= link_to t(:destroy), document, :confirm => t(:confirm), :method => :delete %></td>
  </tr>
<% end %>
<% end %>
